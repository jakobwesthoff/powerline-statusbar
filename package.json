{
  "name": "powerline-statusbar",
  "version": "0.2.0",
  "description": "A powerline inspired statusbar for node cli applications",
  "main": "Distribution/index.js",
  "scripts": {
    "prepublish": "rm -rf ./Distribution/* && node node_modules/.bin/babel -d Distribution index.js && node node_modules/.bin/babel -d Distribution/Library Library",
    "test": "node node_modules/.bin/jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jakobwesthoff/powerline-statusbar.git"
  },
  "keywords": [
    "cli",
    "powerline",
    "statusbar",
    "commandline",
    "inquirer"
  ],
  "author": "Jakob Westhoff <jakob@westhoffswelt.de> (http://westhoffswelt.de)",
  "license": "MIT",
  "dependencies": {
    "cli-width": "^1.0.1",
    "crayon-terminal": "^3.0.0",
    "lodash": "^3.6.0",
    "moment": "^2.10.2"
  },
  "devDependencies": {
    "ansi-regex": "^1.1.1",
    "babel": "^5.0.12",
    "babel-jest": "^5.0.1",
    "jest-cli": "^0.4.0",
    "strip-ansi": "^2.0.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testDirectoryName": "Specification",
    "testFileExtensions": [
      "spec.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "unmockedModulePathPatterns": [
      ".*"
    ]
  }
}
